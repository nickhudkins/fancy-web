{
  "name": "fancy-web-frontend",
  "version": "0.2.0",
  "private": true,
  "devDependencies": {
    "babel-core": "6.18.2",
    "babel-loader": "^6.2.4",
    "babel-preset-react-hmre": "1.1.1",
    "babel-plugin-lodash": "3.2.9",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-object-rest-spread": "^6.19.0",
    "babel-plugin-transform-regenerator": "^6.16.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-watch": "2.0.3",
    "clean-webpack-plugin": "^0.1.8",
    "concurrently": "2.1.0",
    "css-loader": "^0.23.1",
    "eslint": "3.10.2",
    "eslint-loader": "^1.3.0",
    "eventsource-polyfill": "^0.9.6",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "gulp": "3.9.1",
    "gulp-sentry-release": "1.0.2",
    "gulp-util": "3.0.7",
    "json-loader": "^0.5.4",
    "react-hot-loader": "3.0.0-beta.1",
    "react-transform-catch-errors": "1.0.2",
    "redbox-react": "^1.2.4",
    "rimraf": "^2.4.3",
    "strip-loader": "^0.1.2",
    "style-loader": "^0.13.1",
    "webpack": "2.1.0-beta.27",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-dev-server": "2.1.0-beta.11",
    "webpack-hot-middleware": "^2.9.1"
  },
  "dependencies": {
    "babel-polyfill": "6.9.0",
    "babel-relay-plugin": "0.9.3",
    "babel-relay-plugin-loader": "0.8.1",
    "bluebird": "3.4.0",
    "classnames": "^2.2.3",
    "compression": "1.6.2",
    "express": "^4.13.3",
    "glamor": "2.19.0",
    "isomorphic-relay": "^0.7.0",
    "isomorphic-relay-router": "^0.8.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-helmet": "^3.0.0",
    "react-relay": "0.9.3",
    "react-router": "^2.4.0",
    "react-router-relay": "^0.13.2"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack --config .build-tools/webpack.config.prod.js",
    "build:server": "babel src --out-dir dist",
    "start": "npm run build:server && NODE_PATH=./dist NODE_ENV=production node dist/server",
    "start:dev": "concurrently --kill-others 'npm run watch:server' 'npm run watch:client'",
    "watch:server": "NODE_PATH=./src NODE_ENV='development' babel-watch src/server",
    "watch:client": "NODE_ENV='development' node .build-tools/webpack-dev-server.js",
    "release:patch": "npm version patch && npm run release",
    "release:minor": "npm version minor && npm run release",
    "release:major": "npm version major && npm run release",
    "release": "npm run build && gulp --gulpfile ./.build-tools/gulpfile.js sentry:release",
    "deploy": "npm run release && ./build-tools/scripts/deploy.sh"
  },
  "metadata": {
    "graphql": {
      "schema": "./src/relay/schema.json"
    }
  }
}
